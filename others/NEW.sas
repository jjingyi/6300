PROC IML;
 USE FCS; READ ALL INTO Y1;
 USE FBS; READ ALL INTO Y2;
 USE NFL; READ ALL INTO Y3;
 N1=NROW(Y1); N2=NROW(Y2); N3=NROW(Y3);
 V1=N1-1; V2=N2-1; V3=N3-1;
 S1=1/(N1-1)*Y1`*(I(N1)-1/N1*J(N1))*Y1; 
 S2=1/(N2-1)*Y2`*(I(N2)-1/N2*J(N2))*Y2;
 S3=1/(N3-1)*Y3`*(I(N3)-1/N3*J(N3))*Y3;
 SPL=(V1*S1+V2*S2+V3*S3)/(V1+V2+V3);
 DETS1=DET(S1);DETS2=DET(S2);DETS3=DET(S3);DETSPL=DET(SPL);
 MN=(DETS1**(V1/2))*(DETS2**(V2/2))*(DETS3**(V3/2));
 MD=DETSPL**((V1+V2+V2)/2);
 M=MN/MD;
 P=4;
 C1=(((1/V1)+(1/V2)+(1/V3))-1/(V1+V2+V3))*43/60;
 U=-2*(1-C1)*LOG(M);
 PRINT M C1 U ;
 RUN;
 ODS RTF CLOSE;


